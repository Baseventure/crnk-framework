{
	"name": "@crnk/ngrx",
	"version": "0.0.0",
	"license": "MIT",
	"angular-cli": {},
	"scripts": {
		"build:js": "node_modules/.bin/ngc -p tsconfig.dist.json && rimraf build/npm/node_modules",
		"clean:pre": "rimraf build/npm",
		"clean:post": "rimraf \"src/**/*.ngfactory.ts\"",
		"coveralls": "cat coverage/coverage.lcov | ./node_modules/coveralls/bin/coveralls.js",
		"build": "npm run build:js",
		"karma": "karma start",
		"karma:watch": "karma start --no-single-run",
		"lint": "tslint src/**/*.ts",
		"postbuild": "npm run clean:post && npm run copyAssets && npm run copyTemplates",
		"test:unit": "npm run karma",
		"test:unit:watch": "npm run karma:watch",
		"test": "npm run test:unit && npm run lint:lib && npm run coveralls",
		"copyAssets": "ncp assets build/npm/assets",
		"copyTemplates": "ncp component build/npm/component -filter=.*html"
	},
	"main": "./index.js",
	"types": "./index.d.ts",
	"peerDependencies": {
		"ngrx-json-api": ">=1.1.1-adn.1",
		"lodash": ">=4.17.4"
	},
	"devDependencies": {
		"@angular/common": "4.2.6",
		"@angular/animations": "4.2.6",
		"@angular/core": "4.2.6",
		"@angular/forms": "4.2.6",
		"@angular/http": "4.2.6",
		"@angular/router": "4.2.6",
		"@angular/compiler": "4.2.6",
		"@angular/compiler-cli": "4.2.6",
		"@angular/platform-browser": "4.2.6",
		"@angular/platform-browser-dynamic": "4.2.6",
		"@angular/platform-server": "4.2.6",
		"@ngrx/core": "1.2.0",
		"@ngrx/effects": "2.0.3",
		"@ngrx/router-store": "1.2.6",
		"@ngrx/store": "2.2.2",
		"typescript": "2.2.2",
		"lodash": "4.17.4",
		"ngrx-json-api": "1.1.1-adn.7",
		"primeng": "4.0.2",
		"zone.js": "0.8.4",
		"rxjs": "5.2.0",
		"ncp": "2.0.0",
		"rimraf": "2.5.4",
		"@types/lodash": "4.14.61",
		"@types/node": "6.0.41",
		"@types/source-map": "0.1.26"
	}
}

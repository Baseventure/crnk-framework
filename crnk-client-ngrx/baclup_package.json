	"license": "MIT",
	"angular-cli": {},
	"scripts": {
		"build:js": "node_modules/.bin/ngc -p tsconfig.dist.json && rimraf build/npm/node_modules",
		"clean:pre": "rimraf build/npm",
		"clean:post": "rimraf \"src/**/*.ngfactory.ts\"",
		"coveralls": "cat coverage/coverage.lcov | ./node_modules/coveralls/bin/coveralls.js",
		"build": "npm run build:js",
		"karma": "karma start",
		"karma:watch": "karma start --no-single-run",
		"lint": "tslint src/**/*.ts",
		"postbuild": "npm run clean:post && npm run copyAssets && npm run copyTemplates",
		"test:unit": "npm run karma",
		"test:unit:watch": "npm run karma:watch",
		"test": "npm run test:unit && npm run lint:lib && npm run coveralls",
		"copyAssets": "ncp assets build/npm/assets",
		"copyTemplates": "ncp component build/npm/component -filter=.*html"
	},
	"main": "./index.js",
	"types": "./index.d.ts",
